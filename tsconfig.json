{
  "compilerOptions": {
    "declaration": false,
    "jsx": "react",
    "lib": [
      "es6",
      "dom"
    ],
    "typeRoots": [
      "node_modules/@types",
      "@types"
    ],
    "baseUrl": ".",
    "paths": {
      "./*": [
        "./*"
      ]
    },
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noEmitHelpers": false,
    "noEmitOnError": true,
    "strictNullChecks": true,
    "downlevelIteration": true,
    "module": "commonjs",
    "target": "es6"
  }
}