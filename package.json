    "scripts": {
        "postinstall": "ts-node init.ts",
        "start": "ts-node --files index.ts -- --service",
        "start:test": "ts-node -r tsconfig-paths/register test.ts",
        "watch": "nodemon -e ts,yaml,yml -w . --ignore './database/*.json'",
        "build": "tsc --project . --outDir dist && cp tsconfig.json dist/tsconfig.json",
        "test": "mocha --require ts-node/register test/**/*.ts",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ./Config/ormconfig.ts"
    },
